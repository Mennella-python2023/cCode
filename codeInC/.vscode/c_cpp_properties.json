{
    "configurations": [
        {
            "name": "windows-gcc-x86",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "C:/MinGW/bin/gcc.exe",
            "cStandard": "${default}",
            "browse": {
                "limitSymbolsToIncludedHeaders": false,
                "path": [
                    "C:/MinGW/include",
                    "c:/Users/Administrator/Desktop/cCode/codeInC"
                ]
            },
            "compilerPathInCppPropertiesJson": "C:/MinGW/bin/gcc.exe",
            "cppStandard": "${default}",
            "intelliSenseMode": "windows-gcc-x64"
        }
    ],
    "version": 4
}